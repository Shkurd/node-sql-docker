<!DOCTYPE html>
<html lang="en">
  <%- include('partials/header') -%>
<body>
  <%- include('./partials/nav.ejs') %>
  <main>
    <h1>Contacts:</h1>
    <ul>
      <% if (contacts.length) { %>
        <% contacts.forEach(({ link, name }) => { %>
          <li>
            <a href="<%= link %>"><%= name %></a>
          </li>
        <% }) %>
      <% } %>
    </ul>
  </main>
  <%- include('./partials/footer.ejs') %>
</body>
</html>